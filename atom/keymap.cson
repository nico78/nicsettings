'.editor:not([mini])':
   'alt-y': 'find-and-replace:select-next'
   'ctrl-alt-y': 'find-and-replace:select-skip'
   'alt-shift-y': 'find-and-replace:select-undo'
   'alt-shift-up': 'expand-region:expand'
   'alt-shift-down': 'expand-region:shrink'
   'ctrl-shift-P': 'command-palette:toggle'
   'ctrl-shift-up': 'editor:add-selection-above'
   'ctrl-shift-down': 'editor:add-selection-below'

'.platform-win32 atom-text-editor:not([mini]), .platform-linux atom-text-editor:not([mini])':
  'ctrl-shift-P': 'command-palette:toggle'
